{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "dev"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^main\\.nf$"
      ],
      "matchStrings": [
        "container\\s+['\"]docker:\\/\\/(?<depName>.*?):(?<currentValue>.*?)['\"]"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
